{
  "expo": {
    "name": "RecruitFlow",
    "slug": "recruitflowproject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Recruitflow_1-removebg-preview.png",
    "scheme": "recruitflowproject",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.recruitflow.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Recruitflow_1-removebg-preview.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#0D9494",
          "dark": {
            "backgroundColor": "#0D9494"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3c29c5cc-ccac-40eb-a41a-65578909e8c5"
      }
    }
  }
}
